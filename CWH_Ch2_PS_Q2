import java.util.Scanner;
public class CWH_Ch2_PS_Q2 {
    public static void main(String[] args) {

        //Question2
        char grade = 'B';
        grade = (char)(grade + 8);
        System.out.println(grade);
        //Decrypting the grade
        grade = (char)(grade - 8);
        System.out.println(grade);
    }
}

